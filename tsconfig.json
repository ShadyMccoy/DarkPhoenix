{
  "compilerOptions": {
    "target": "es2019",
    "moduleResolution": "node",
    "outDir": "dist/",
    "baseUrl": "src/",
    "sourceMap": true,
    "strict": true,
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "lib": [
      "es2019"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
